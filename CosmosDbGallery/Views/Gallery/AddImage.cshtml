
@{
    ViewData["Title"] = "AddImage";
}

<h1>Add Image</h1>
<form method="post" enctype="multipart/form-data" asp-controller="Gallery" asp-action="AddImage">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload one or more files using this form:</p>
            <input type="file" id="files" name="files" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
